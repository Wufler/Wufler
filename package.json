{
	"name": "wolfey",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev --turbopack",
		"build": "npx prisma generate && npx prisma migrate deploy && next build --turbopack",
		"start": "next start",
		"lint": "eslint",
		"db:start": "docker-compose up -d",
		"db:stop": "docker-compose down",
		"db:seed": "npx prisma db seed",
